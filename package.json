    "@mui/material": "^5.10.6",
    "@mui/system": "^5.10.6",
    "@mui/utils": "^5.10.6",
    "@mui/x-data-grid": "^7.6.2",
    "@mui/x-date-pickers": "^7.3.1",
    "@prisma/client": "^6.13.0",
    "@supabase/supabase-js": "^2.50.2",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "firebase": "^10.10.0",
    "is-mobile": "^3.1.1",
    "notistack": "^3.0.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
