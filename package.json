  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "scripts": {
    "test": "vitest --pool=forks",
    "test:e2e": "npx playwright test --headed",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "prepare": "husky install",
    "deploy": "./scripts/env-manager.sh prod && pnpm run build && firebase deploy && curl -X PURGE https://mercado-fiel.web.app/*",
    "emulators": "firebase emulators:start --only functions,hosting",
    "supabaseEmulators": "supabase start emulators && cd ./functions && node ./lib/seed.js",
    "env:setup": "chmod +x ./scripts/env-manager.sh && ./scripts/env-manager.sh setup",
    "env:dev": "./scripts/env-manager.sh dev",
    "env:prod": "./scripts/env-manager.sh prod",
    "env:status": "./scripts/env-manager.sh status",
    "env:clean": "./scripts/env-manager.sh clean",
    "dev:start": "./scripts/env-manager.sh start",
    "dev:quick": "pnpm run env:dev && pnpm run dev",
    "prod:deploy": "./scripts/env-manager.sh deploy && curl -X PURGE https://mercado-fiel.web.app/",
    "prod:quick": "pnpm run env:prod && pnpm run build && firebase deploy && curl -X PURGE https://mercado-fiel.web.app/",
    "lint": "eslint --ext .js,.ts,.tsx src"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@import-meta-env/unplugin": "^0.5.1",
