{
  "name": "mercado-fiel",
  "version": "0.1",
  "description": "Plataforma marketplace para conectar compradores con proveedores de productos y servicios.",
  "homepage": "TODO",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "TODO"
  },
  "bugs": {
    "url": "https://www.TODO.com/todo",
    "email": "soporte@mercadofiel.com"
